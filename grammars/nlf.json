{
  "fileTypes": [
    "nlf"
  ],
  "name": "NSIS Language File",
  "comment": "https://github.com/idleberg/atom-language-nsis",
  "patterns": [
    {
      "match": "^\\s*#.*$\\n?",
      "name": "comment.line.nlf"
    },
    {
      "match": "\\b((0(x|X)[0-9a-fA-F]+)|([0-9]+(\\.[0-9]+)?))\\b",
      "name": "constant.numeric.nlf"
    },
    {
      "match": "(?i)\\$_CLICK",
      "name": "variable.language.nlf"
    },
    {
      "match": "\\$\\w+",
      "name": "variable.other.nlf"
    },
    {
      "match": "\\%\\w+",
      "name": "constant.language.nlf"
    },
    {
      "match": "(\\w*?&\\w+)",
      "name": "constant.character.nlf"
    },
    {
      "match": "\\$\\([\\w\\.:\\^-]+\\)",
      "name": "constant.other.nlf"
    },
    {
      "match": "\\\\.",
      "name": "constant.character.escape.nlf"
    }
  ],
  "scopeName": "source.nlf"
}
