      : ${10:License Agreement}
      # ^ComponentsSubCaption
      : ${11:Installation Options}
      # ^DirSubCaption
      : ${12:Installation Folder}
      # ^InstallingSubCaption
      : ${13:Installing}
      # ^CompletedSubCaption
      : ${14:Completed}
      # ^UnComponentsSubCaption
      : ${15:Uninstallation Options}
      # ^UnDirSubCaption
      : ${16:Uninstallation Folder}
