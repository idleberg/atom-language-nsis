    "makensis": "^0.16.1",
    "opn": "^5.4.0",
    "yaml-js": "^0.3.0"
  },
  "devDependencies": {
    "coffeelint-stylish": "^0.1.2",
    "gulp": "^4.0.0",
    "gulp-coffeelint": "^0.6.0",
    "gulp-debug": "^4.0.0",
    "gulp-jsonlint": "^1.2.2",
    "husky": "^0.14.3"
  }
}
