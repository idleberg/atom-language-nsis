{
  "name": "language-nsis",
  "version": "6.18.2",
  "description": "Atom language support for NSIS, including grammar, snippets and build system",
  "license": "MIT OR GPL-2.0",
  "scripts": {
    "precommit": "npm run test",
    "test": "gulp lint"
  },
  "author": {
    "name": "Jan T. Sott",
    "url": "http://github.com/idleberg"
  },
  "keywords": [
    "nsis",
    "nullsoft",
    "installer",
    "setup",
    "windows",
    "build",
    "build tool",
    "compiler",
    "grammar",
    "language syntax"
  ],
  "repository": {
