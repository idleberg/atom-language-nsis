{
  "name": "language-nsis",
  "version": "5.16.2",
  "description": "Atom language support for NSIS, including grammar, snippets and build system",
  "license": "MIT OR GPL-2.0",
  "author": {
    "name": "Jan T. Sott",
    "url": "http://github.com/idleberg"
  },
  "keywords": [
    "nsis",
    "nullsoft",
    "installer",
    "setup",
    "windows",
    "build",
    "build tool",
    "compiler",
    "grammar",
    "language syntax"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/idleberg/atom-language-nsis.git"
  },
  "main": "./lib/main",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "activationCommands": {
    "atom-workspace": [
      "NSIS:save-&-compile",
      "NSIS:save-&-compile-strict",
      "NSIS:show-version",
      "NSIS:open-package-settings",
      "NSIS:create-.atom–build-file",
      "NSIS:create-.atom–build-file-for-wine",
      "NSIS:set-default-runner",
      "NSIS:remove-default-runner"
    ]
  },
  "package-deps": [
    "console-panel"
  ],
  "consumedServices": {
    "console-panel": {
      "versions": {
        "^1.0.0": "consumeConsolePanel"
      }
    }
  },
  "dependencies": {
    "atom-package-deps": "^4.4.1",
    "cson": "^3.0.2",
    "jshint": "^2.9.3",
    "yaml-js": "^0.1.4"
  },
  "devDependencies": {
    "coffeelint-stylish": "^0.1.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-coffeelint": "^0.6.0",
    "gulp-debug": "^3.1.0",
    "gulp-jshint": "^2.0.1",
    "gulp-jsonlint": "^1.1.2",
    "gulp-lesshint": "^3.0.0",
    "jshint": "^2.9.3"
  },
  "scripts": {
    "test": "gulp lint"
  }
}
