{
  "name": "language-nsis",
  "version": "8.19.4",
  "description": "Atom language support for NSIS, including grammar, snippets and build system",
  "license": "MIT",
  "main": "./lib/main",
  "typings": "./types/index.d.ts",
  "scripts": {
    "build": "npm run clean && rollup --config",
    "clean": "rimraf ./lib",
    "dev": "npm run start",
    "lint:md": "remark . --quiet --frail --ignore-path .gitignore",
    "lint:ts": "eslint ./src --ignore-path .gitignore",
    "lint": "npm-run-all --parallel lint:*",
    "prepublishOnly": "npm run clean && npm run build",
    "start": "rollup --watch --config",
    "test": "npm run lint",
    "prepare": "husky install"
  },
  "author": {
    "name": "Jan T. Sott",
    "url": "http://github.com/idleberg"
  },
  "keywords": [
    "nsis",
    "nullsoft",
    "installer",
    "setup",
    "windows",
    "build",
    "build tool",
    "compiler",
    "grammar",
    "language syntax"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/atom-language-nsis.git"
  },
  "engines": {
    "atom": ">=1.52.0 <2.0.0"
  },
  "package-deps": [
    {
      "name": "browse",
      "minimumVersion": "3.1.0"
    },
    {
      "name": "busy-signal",
      "minimumVersion": "2.0.0"
    },
    [
      {
        "name": "console",
        "minimumVersion": "0.4.0"
      },
      {
        "name": "console-panel"
      }
    ],
    {
      "name": "atom-ide-hyperclick"
    },
    {
      "name": "hyperclick-nsis",
      "minimumVersion": "0.5.0"
    },
    {
      "name": "linter",
      "minimumVersion": "2.0.0"
    },
    {
      "name": "linter-makensis",
      "minimumVersion": "0.7.0"
    },
    {
      "name": "linter-ui-default"
    },
    [
      {
        "name": "linter"
      },
      {
        "name": "atom-ide-ui"
      }
    ]
  ],
  "activationCommands": {
    "atom-workspace": [
      "NSIS:command-reference",
      "NSIS:compile",
      "NSIS:compile-strict",
      "NSIS:convert-language-file",
      "NSIS:create-.atom-build-file",
      "NSIS:open-package-settings",
      "NSIS:satisfy-dependencies",
      "NSIS:set-default-runner",
      "NSIS:show-compiler-flags",
      "NSIS:show-version",
      "NSIS:unset-default-runner"
    ]
  },
  "activationHooks": [
    "language-nlf:grammar-used",
    "language-nsis:grammar-used"
  ],
  "consumedServices": {
    "browse": {
      "versions": {
        "^1.0.0": "consumeBrowse"
      }
    },
    "busy-signal": {
      "versions": {
        "1.0.0": "consumeSignal"
      }
    },
    "console": {
      "versions": {
        "^1.0.0": "consumeConsolePanel"
      }
    },
    "console-panel": {
      "versions": {
        "^1.0.0": "consumeConsolePanel"
      }
    }
  },
  "devDependencies": {
    "@atxm/developer-console": "^0.5.0",
    "@nsis/nlf": "^0.10.2",
    "@rollup/plugin-commonjs": "^23.0.7",
    "@rollup/plugin-json": "^5.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.1.0",
    "@rollup/plugin-typescript": "^9.0.2",
    "@stylelint/remark-preset": "^4.0.0",
    "@types/atom": "^1.40.11",
    "@types/node": "^16.18.11",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "atom-package-deps": "^8.0.0",
    "atom-satisfy-dependencies": "^0.3.0",
    "atom-select-list": "^0.8.1",
    "eslint": "^8.32.0",
    "eslint-plugin-json": "^3.1.0",
    "execa": "^5.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "makensis": "^1.1.4",
    "npm-run-all": "^4.1.5",
    "open": "^8.4.0",
    "prettier": "^2.8.3",
    "remark-cli": "^11.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^3.11.0",
    "stylelint": "^14.16.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.4",
    "which": "^3.0.0",
    "yaml": "^2.2.1"
  },
  "remarkConfig": {
    "plugins": [
      "@stylelint/remark-preset"
    ]
  },
  "lint-staged": {
    "*.(json|ts)": "eslint --cache --fix",
    "*.md": "prettier --write"
  }
}
