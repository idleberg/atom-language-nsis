{
  "name": "language-nsis",
  "version": "8.10.1",
  "description": "Atom language support for NSIS, including grammar, snippets and build system",
  "license": "MIT OR GPL-2.0",
  "main": "./lib/main",
  "typings": "./types/index.d.ts",
  "scripts": {
    "build": "npm run clean && rollup --config",
    "clean": "rimraf ./lib",
    "dev": "npm run start",
    "lint:json": "jsonlint ./grammars/*.json ./keymaps/*.json menus/*.json snippets/*.json --quiet",
    "lint:md": "remark . --quiet --frail --ignore-path .gitignore",
    "lint:ts": "eslint ./src --ignore-path .gitignore",
    "lint": "npm-run-all --parallel lint:*",
    "prepublishOnly": "npm run clean && npm run build",
    "start": "rollup --watch --config",
    "test": "npm run lint"
  },
  "author": {
    "name": "Jan T. Sott",
    "url": "http://github.com/idleberg"
  },
  "keywords": [
    "nsis",
    "nullsoft",
    "installer",
    "setup",
    "windows",
    "build",
    "build tool",
    "compiler",
    "grammar",
    "language syntax"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/atom-language-nsis.git"
  },
  "engines": {
    "atom": ">=1.19.0 <2.0.0"
  },
  "package-deps": [
    {
      "name": "browse",
      "minimumVersion": "3.1.0"
    },
    {
      "name": "busy-signal",
      "minimumVersion": "2.0.0"
    },
    [
      {
        "name": "console",
        "minimumVersion": "0.4.0"
      },
      {
        "name": "console-panel"
      }
    ],
    {
      "name": "atom-ide-hyperclick"
    },
    {
      "name": "hyperclick-nsis",
      "minimumVersion": "0.5.0"
    },
    {
      "name": "linter",
      "minimumVersion": "2.0.0"
    },
    {
      "name": "linter-makensis",
      "minimumVersion": "0.7.0"
    },
    {
      "name": "linter-ui-default"
    },
    [
      {
        "name": "linter"
      },
      {
        "name": "atom-ide-ui"
      }
    ]
  ],
  "activationCommands": {
    "atom-workspace": [
      "NSIS:command-reference",
      "NSIS:compile",
      "NSIS:compile-strict",
      "NSIS:convert-language-file",
      "NSIS:create-.atomâ€“build-file",
      "NSIS:open-package-settings",
      "NSIS:satisfy-dependencies",
      "NSIS:set-default-runner",
      "NSIS:show-compiler-flags",
      "NSIS:show-version",
      "NSIS:unset-default-runner"
    ]
  },
  "activationHooks": [
    "language-nlf:grammar-used",
    "language-nsis:grammar-used"
  ],
  "consumedServices": {
    "browse": {
      "versions": {
        "^1.0.0": "consumeBrowse"
      }
    },
    "busy-signal": {
      "versions": {
        "1.0.0": "consumeSignal"
      }
    },
    "console": {
      "versions": {
        "^1.0.0": "consumeConsolePanel"
      }
    },
    "console-panel": {
      "versions": {
        "^1.0.0": "consumeConsolePanel"
      }
    }
  },
  "dependencies": {
    "@atxm/developer-console": "^0.5.0",
    "@nsis/nlf": "^0.9.0",
    "atom-package-deps": "^7.0.3",
    "atom-satisfy-dependencies": "^0.3.0",
    "atom-select-list": "^0.8.0",
    "dotenv": "^8.2.0",
    "execa": "^5.0.0",
    "makensis": "^0.23.3",
    "open": "^8.0.2",
    "which": "^2.0.2",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-typescript": "^8.0.0",
    "@stylelint/remark-preset": "^2.0.0",
    "@types/atom": "^1.40.4",
    "@types/node": "^14.14.16",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "eslint": "^7.16.0",
    "eslint-plugin-json": "^2.1.2",
    "husky": ">=4.3 <5",
    "jsonlint": "^1.6.3",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "remark-cli": "^9.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.35.1",
    "rollup-plugin-terser": "^7.0.2",
    "stylelint": "^13.12.0",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  },
  "remarkConfig": {
    "plugins": [
      "@stylelint/remark-preset"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.json": "jsonlint --quiet",
    "*.md": "prettier --write"
  }
}
